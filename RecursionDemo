//Print the 1 to 10 no using Recursion
//if num =10


public class RecursionDemo {


    void numPrint(int num){
        if(num ==0){
            return;
        }

        numPrint(num-1);
        System.out.println(num);



    }

    public static void main(String[]args){

        RecursionDemo obj = new RecursionDemo();

        obj.numPrint(10);

    }
}
