//Double Recursion


public class RecursionDouble1{
    void fun(int num){
        if(num<=1)
            return ;
        fun(num-2);
        System.out.println(num);
        fun(num-1);
    }
    public static void main(String[]args){
        RecursionDouble1 obj = new RecursionDouble1();
        obj.fun(5);

    }



}




